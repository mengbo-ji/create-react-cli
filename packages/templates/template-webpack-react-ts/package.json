    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "openurl": "^1.1.1",
    "portfinder": "^1.0.28",
    "stylelint": "^9.3.0",
    "stylelint-config-css-modules": "^1.5.0",
    "stylelint-config-recommended": "^2.2.0",
    "typescript": "^3.7.0"
  },
  "git-pre-hooks": {
    "pre-commit": "npm run lint"
  }
