    "build": "npm run clean && react-builder-rc build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix --cache",
    "server": "node builder/server.js"
  },
  "builder-config": {
    "typescript": true
  },
  "dependencies": {
    "@ant-design/charts": "^1.1.4",
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^6.22.1",
    "antd": "4.17.0",
    "dva": "^2.6.0-beta.22",
    "dva-loading": "^3.0.22",
    "history": "^4.10.1",
    "immer": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "query-string": "^7.0.0",
    "react": "^17.0.0",
    "react-builder-rc": "^2.0.1",
    "react-dom": "^17.0.0",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^5.2.3",
    "umi-request": "^1.3.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "chalk": "^4.1.1",
    "eslint": "^7.27.0",
